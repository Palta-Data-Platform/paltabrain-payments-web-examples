{
    "name": "react-webpack-typescript-starter",
    "version": "0.1.0",
    "description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
    "keywords": [
        "react",
        "webpack",
        "typescript",
        "babel",
        "sass",
        "hmr",
        "starter",
        "boilerplate"
    ],
    "author": "Viktor Persson",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vikpe/react-webpack-typescript-starter.git"
    },
    "bugs": {
        "url": "https://github.com/vikpe/react-webpack-typescript-starter/issues"
    },
    "homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
    "scripts": {
        "build": "yarn run clean-dist && webpack --config=configs/webpack/prod.js",
        "clean-dist": "rimraf dist/*",
        "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
        "start": "yarn run start-dev",
        "start-dev": "webpack serve --config=configs/webpack/dev.js",
        "start-prod": "yarn run build",
        "test": "jest --coverage --watchAll --config=configs/jest.json"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.3",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.21.0",
        "@types/jest": "^29.4.2",
        "@types/node": "^18.15.3",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/react-router-dom": "^5.3.3",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.55.0",
        "@typescript-eslint/parser": "^5.55.0",
        "babel-loader": "^9.1.2",
        "body-parser": "^1.20.2",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.3",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.7.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "express": "^4.18.2",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "image-webpack-loader": "^8.1.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hot-loader": "^4.13.1",
        "regenerator-runtime": "^0.13.11",
        "rimraf": "^4.4.0",
        "sass-loader": "^13.2.0",
        "style-loader": "^3.3.2",
        "typescript": "^4.9.5",
        "webpack": "^5.76.1",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.12.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@palta-brain/payments": "^0.4.0",
        "@primer-io/checkout-web": "^2.27.2",
        "antd": "^5.6.3",
        "react-router-dom": "^6.9.0",
        "sass": "^1.62.0",
        "uuid": "^9.0.0"
    }
}
